@using System.Collections.Generic
@using PracticaAlmacenes.Models
@model System.Collections.Generic.IEnumerable<PracticaAlmacenes.Models.Etiqueta>


@{
    ViewBag.Title = "title";
    var alm = ViewBag.Almacenes as IEnumerable<Almacen>;

}
<div class="col-md-9">
    @foreach (var item in Model)
    {
        <div class="col-md-4">
            <h2>@item.descripcion</h2>
            <ul>
                @foreach (var pro in item.Producto)
                {
                    <li>@pro.nombre</li>
                }
            </ul>
        </div>

    }
</div>
<div class="col-md-3">
    <h1>Almacenes</h1>
    <ul>
        @foreach (var almacen in alm)
        {
            <li>@almacen.nombre</li>
        }
    </ul>
</div>